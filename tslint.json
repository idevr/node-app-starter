{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [true, "always"]
  },
  "rulesDirectory": []
}
